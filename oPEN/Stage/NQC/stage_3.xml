<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
<Output>
      <!-- 出力言語の設定ファイルへのパス
    	出力現をを設定しないならば(NULL)を記載 -->
    <Language>NQC.xml</Language>
    
    <!-- 拡張子 -->
    <FileNameExtention>.nqc</FileNameExtention>
    
    <!-- ソースコード出力ボタンの有無(ON or OFF) -->
    <OutputButton>ON</OutputButton>
</Output>
<BlockDrawerSets>
<BlockDrawerSet drawer-draggable="no" location="southwest" name="factory" type="stack" window-per-drawer="no">
			<BlockDrawer name="制御系" type="factory" button-color="255 173 0">
			    <BlockGenusMember>start</BlockGenusMember>  
			    <BlockGenusMember>if</BlockGenusMember>
			    <BlockGenusMember>repeat</BlockGenusMember>  
			    <BlockGenusMember>and</BlockGenusMember>
				<BlockGenusMember>or</BlockGenusMember>
				<BlockGenusMember>not</BlockGenusMember>
				<BlockGenusMember>equals</BlockGenusMember>
				<BlockGenusMember>lessthan</BlockGenusMember>
				<BlockGenusMember>true</BlockGenusMember>
				<BlockGenusMember>false</BlockGenusMember>
			</BlockDrawer>
			
			<BlockDrawer name="変数と四則演算" type="factory" button-color="50 50 255">
				<BlockGenusMember>number</BlockGenusMember>
				<BlockGenusMember>setInt</BlockGenusMember>
				<BlockGenusMember>substitution-number</BlockGenusMember>
				<BlockGenusMember>variable-Number</BlockGenusMember>
				<BlockGenusMember>sum</BlockGenusMember>
				<BlockGenusMember>difference</BlockGenusMember>
				<BlockGenusMember>quotient</BlockGenusMember>
				<BlockGenusMember>product</BlockGenusMember>
				<BlockGenusMember>surplus</BlockGenusMember>
			</BlockDrawer>
			
			<BlockDrawer  name="モーター" type="factory" button-color="255 50 50">
				<BlockGenusMember>out_bc</BlockGenusMember>
				<BlockGenusMember>motor-on-fwd</BlockGenusMember>
				<BlockGenusMember>motor-on-a</BlockGenusMember>
				<BlockGenusMember>motor-off</BlockGenusMember>
				<BlockGenusMember>wait</BlockGenusMember>
				<BlockGenusMember>rotate</BlockGenusMember>
			</BlockDrawer>
			
			<BlockDrawer name="センサー" type="factory" button-color="255 0 173">
				<BlockGenusMember>sensor-input-port3</BlockGenusMember>
				<BlockGenusMember>set-touch-sensor</BlockGenusMember>
			</BlockDrawer>
</BlockDrawerSet>
</BlockDrawerSets>
</root>