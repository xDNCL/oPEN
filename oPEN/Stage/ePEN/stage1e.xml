<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
<Output>
      <!-- 出力言語の設定ファイルへのパス
    	出力現をを設定しないならば(NULL)を記載 -->
    <Language>PEN.xml</Language>
    
    <!-- 拡張子 -->
    <FileNameExtention>.pen</FileNameExtention>
    
    <!-- ソースコード出力ボタンの有無(ON or OFF) -->
    <OutputButton>ON</OutputButton>
</Output>
<BlockDrawerSets>
<BlockDrawerSet drawer-draggable="no" location="southwest" name="factory" type="stack" window-per-drawer="no">
			<BlockDrawer name="Control" type="factory" button-color="255 173 0">
			    <BlockGenusMember>e-start</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="Declaration・Assignment" button-color="132 204 201" type="factory">
				<BlockGenusMember>e-setInt</BlockGenusMember>
				<BlockGenusMember>e-substitution-number</BlockGenusMember>
			    <BlockGenusMember>e-number</BlockGenusMember>
			    <BlockGenusMember>e-variable-Number</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="Operators" type="factory" button-color="102 129 255">
				<BlockGenusMember>e-sum</BlockGenusMember>
				<BlockGenusMember>e-difference</BlockGenusMember>
				<BlockGenusMember>e-quotient</BlockGenusMember>
				<BlockGenusMember>e-product</BlockGenusMember>
				<BlockGenusMember>e-surplus</BlockGenusMember>
			    <BlockGenusMember>e-number</BlockGenusMember>
			    <BlockGenusMember>e-variable-Number</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="Input・Output" button-color="255 255 255" type="factory">
				<BlockGenusMember>e-print-number</BlockGenusMember>
				<BlockGenusMember>e-println-number</BlockGenusMember>
				<BlockGenusMember>e-keyboard-input-number</BlockGenusMember>
			    <BlockGenusMember>e-number</BlockGenusMember>
			    <BlockGenusMember>e-variable-Number</BlockGenusMember>
			</BlockDrawer>
			<!-- BlockDrawer name="関数" type="factory" button-color="177 10 255">
			    <BlockGenusMember>procedure</BlockGenusMember>
			    <BlockGenusMember>proc-param-number</BlockGenusMember>
			    <BlockGenusMember>proc-param-boolean</BlockGenusMember>
			    <BlockGenusMember>proc-param-string</BlockGenusMember>
			</BlockDrawer -->    
</BlockDrawerSet>
</BlockDrawerSets>
</root>