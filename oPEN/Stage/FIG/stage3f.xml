<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<root>
<Output>
      <!-- 出力言語の設定ファイルへのパス
    	出力現をを設定しないならば(NULL)を記載 -->
    <Language>PEN.xml</Language>
    
    <!-- 拡張子 -->
    <FileNameExtention>.pen</FileNameExtention>
    
    <!-- ソースコード出力ボタンの有無(ON or OFF) -->
    <OutputButton>ON</OutputButton>
</Output>
<BlockDrawerSets>
<BlockDrawerSet drawer-draggable="no" location="southwest" name="factory" type="stack" window-per-drawer="no">
			<BlockDrawer name="制御" type="factory" button-color="255 173 0">
			    <BlockGenusMember>start</BlockGenusMember>
			    <BlockGenusMember>if</BlockGenusMember>
			    <BlockGenusMember>ifelse</BlockGenusMember>
			    <BlockGenusMember>repeat</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="宣言・代入" button-color="132 204 201" type="factory">
			    <BlockGenusMember>setInt</BlockGenusMember>
			    <BlockGenusMember>substitution-number</BlockGenusMember>
			    <BlockGenusMember>number</BlockGenusMember>
			    <BlockGenusMember>variable-Number</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="四則演算" type="factory" button-color="102 129 255">
				<BlockGenusMember>sum</BlockGenusMember>
				<BlockGenusMember>difference</BlockGenusMember>
				<BlockGenusMember>quotient</BlockGenusMember>
				<BlockGenusMember>product</BlockGenusMember>
				<BlockGenusMember>surplus</BlockGenusMember>
			    	<BlockGenusMember>number</BlockGenusMember>
			    	<BlockGenusMember>variable-Number</BlockGenusMember>
			    	<BlockGenusMember>string</BlockGenusMember>
			    	<BlockGenusMember>xy</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="入出力" button-color="255 255 255" type="factory">
				<BlockGenusMember>print-number</BlockGenusMember>
				<BlockGenusMember>println-number</BlockGenusMember>
				<BlockGenusMember>keyboard-input-number</BlockGenusMember>
			    	<BlockGenusMember>number</BlockGenusMember>
			    	<BlockGenusMember>variable-Number</BlockGenusMember>
				<BlockGenusMember>string</BlockGenusMember>
				<BlockGenusMember>xy</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="図形属性" type="factory" button-color="200 255 0">
			    <BlockGenusMember>number</BlockGenusMember>
			    <BlockGenusMember>xy</BlockGenusMember>
			    <BlockGenusMember>gSetFillColor</BlockGenusMember>
			    <BlockGenusMember>gSetTextColor</BlockGenusMember>
 			    <BlockGenusMember>gSetFont</BlockGenusMember>
			    <BlockGenusMember>gSetFontType</BlockGenusMember>
 			    <BlockGenusMember>gSetFontSize</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="図形描画" type="factory" button-color="0 255 0">
			    <BlockGenusMember>number</BlockGenusMember>
			    <BlockGenusMember>xy</BlockGenusMember>
			    <BlockGenusMember>gDrawBox</BlockGenusMember>
			    <BlockGenusMember>gFillBox</BlockGenusMember>
			    <BlockGenusMember>gDrawCircle</BlockGenusMember>
			    <BlockGenusMember>gFillCircle</BlockGenusMember>
			    <BlockGenusMember>gDrawText</BlockGenusMember>
			</BlockDrawer>
			<BlockDrawer name="ウィンドウ操作" type="factory" button-color="0 80 0">
 			    <BlockGenusMember>gOpenWindow</BlockGenusMember>
			</BlockDrawer>
</BlockDrawerSet>
</BlockDrawerSets>
</root>