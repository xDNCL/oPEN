			    <BlockGenusMember>start</BlockGenusMember>
			</BlockDrawer>

			<BlockDrawer  name="モーター" type="factory" button-color="255 50 50">
				<BlockGenusMember>out_bc</BlockGenusMember>
				<BlockGenusMember>motor-on-fwd</BlockGenusMember>
				<BlockGenusMember>motor-on-b</BlockGenusMember>
				<BlockGenusMember>motor-off</BlockGenusMember>
				<BlockGenusMember>wait</BlockGenusMember>
				<BlockGenusMember>rotate</BlockGenusMember>
			</BlockDrawer>
</BlockDrawerSet>
</BlockDrawerSets>
</root>